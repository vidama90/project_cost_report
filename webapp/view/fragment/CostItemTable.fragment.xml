<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:table="sap.ui.table"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
>
    <VBox
        visible="{ViewControl>/Tables/ShowCostItem}"
        width="100%"
    >
        <Toolbar>
            <ToolbarSpacer />
            <Button
                icon="sap-icon://personnel-view"
                press=".onCostItemTablePersonalizePress"
                tooltip="Personalize Table"
            />
        </Toolbar>
        <table:Table
            id="CostItemTable"
            visible="{ViewControl>/Tables/ShowCostItem}"
            selectionMode="None"
            enableColumnFreeze="true"
            fixedColumnCount="3"
            rowsUpdated=".onTableRowsUpdated"
            class="mainTable"
            rows="{
                path: 'to_CostItem',
                parameters: {
                    groupId: 'costChanges'
                },
                sorter: {
                    path: 'counter',
                    ascending: true
                }
            }"
        >
            <table:rowSettingsTemplate>
                <table:RowSettings
                    highlight="{= 
                        ${HierarchyLevel}===2 ? 'Information' : 
                        ${HierarchyLevel}===3 ? 'Warning' : 
                        ${HierarchyLevel}===4||${HierarchyLevel}===5 ? 'Success' : 
                        'None'
                    }"
                />
            </table:rowSettingsTemplate>

            <table:columns>
                <table:Column
                    id="CostItemTableColumnWBSLevel2Descr"
                    width="6rem"
                >
                    <table:label>
                        <Label
                            text="{/#ProjectCostItemDataType/WBSLevel2Descr/@sap:label}"
                            textAlign="Left"
                        />
                    </table:label>
                    <table:template>
                        <HBox>
                            <Text
                                text="{WBSLevel2Descr}"
                                class="emphasizedColumn"
                                visible="{= ${HierarchyLevel} !== 5 &amp;&amp; ${HierarchyLevel} !== 6 &amp;&amp; ${HierarchyLevel} !== 7}"
                            />
                                <Text
                                    text="Sub Total:-"
                                    textAlign="Right"
                                    class="hierarchyLevel6"
                                    visible="{= ${HierarchyLevel} === 6}"
                                />
                                <Text
                                    text="Total:-"
                                    textAlign="Right"
                                    class="hierarchyLevel7"
                                    visible="{= ${HierarchyLevel} === 7}"
                                />
                        </HBox>
                    </table:template>
                </table:Column>
                <table:Column
                    id="CostItemTableColumnWBSLevel3Descr"
                    width="8rem"
                >
                    <table:label>
                        <Label
                            text="{/#ProjectCostItemDataType/WBSLevel3Descr/@sap:label}"
                            textAlign="Left"
                        />
                    </table:label>
                    <table:template>
                        <HBox>
                            <Text
                                text="{WBSLevel3Descr}"
                                class="emphasizedColumn"
                                visible="{= ${HierarchyLevel} !== 5 &amp;&amp; ${HierarchyLevel} !== 6 &amp;&amp; ${HierarchyLevel} !== 7}"
                            />
                            <Text
                                text="{WBSLevel3Descr}"
                                class="emphasizedColumn hierarchyLevel6"
                                visible="{= ${HierarchyLevel} === 6}"
                            />
                            <Text
                                text="{WBSLevel3Descr}"
                                class="emphasizedColumn hierarchyLevel7"
                                visible="{= ${HierarchyLevel} === 7}"
                            />
                        </HBox>
                    </table:template>
                </table:Column>
                <table:Column
                    id="CostItemTableColumnWBSLevel4Descr"
                    width="6rem"
